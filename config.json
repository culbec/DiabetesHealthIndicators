{
    "DiabetesHealthIndicators": {
        "loader": {
            "dataset_type": "dhi",
            "dataset_path": "../.datasets/diabetes_health_indicators/diabetes_dataset.csv",
            "separator": ",",
            "decimal": ".",
            "label_columns": [
                "diabetes_stage",
                "diagnosed_diabetes",
                "diabetes_risk_score"
            ]
        },
        "preprocessor": {
            "dataset_type": "dhi",
            "numerical_features": {
                "scaler": {
                    "type": "robust_scaler",
                    "params": {}
                },
                "features": [
                    "age",
                    "alcohol_consumption_per_week",
                    "physical_activity_minutes_per_week",
                    "diet_score",
                    "sleep_hours_per_day",
                    "screen_time_hours_per_day",
                    "bmi",
                    "waist_to_hip_ratio",
                    "systolic_bp",
                    "diastolic_bp",
                    "heart_rate",
                    "cholesterol_total",
                    "hdl_cholesterol",
                    "ldl_cholesterol",
                    "triglycerides",
                    "glucose_fasting",
                    "glucose_postprandial",
                    "insulin_level",
                    "hba1c",
                    "diabetes_risk_score"
                ]
            },
            "categorical_features": {
                "encoder": {
                    "type": "label_encoder",
                    "params": {}
                },
                "features": [
                    "gender",
                    "ethnicity",
                    "education_level",
                    "income_level",
                    "family_history_diabetes",
                    "hypertension_history",
                    "cardiovascular_history",
                    "employment_status",
                    "smoking_status",
                    "diabetes_stage",
                    "diagnosed_diabetes"
                ]
            },
            "nan_category": "Unknown",
            "dry_run": false,
            "numerical_to_categorical_features": {
                "family_history_diabetes": {
                    "0": "No",
                    "1": "Yes"
                },
                "hypertension_history": {
                    "0": "No",
                    "1": "Yes"
                },
                "cardiovascular_history": {
                    "0": "No",
                    "1": "Yes"
                },
                "diagnosed_diabetes": {
                    "0": "No",
                    "1": "Yes"
                }
            }
        },
        "ml": {
            "svr_sklearn": {
                "model_name": "svr_sklearn",
                "save_path": "../.models/svr_sklearn",
                "metrics_path": "../.models/svr_sklearn",
                "cv_statistics_path": "../.models/svr_sklearn",
                "params": {
                    "kernel": "rbf",
                    "degree": 3,
                    "coef0": 0.0,
                    "tol": 1e-3,
                    "C": 1.0,
                    "epsilon": 1e-1,
                    "max_iter": -1
                },
                "cv_params": {
                    "param_grid": [
                        {
                            "kernel": [
                                "linear"
                            ],
                            "C": [
                                0.5,
                                0.75,
                                1,
                                1.5
                            ]
                        },
                        {
                            "kernel": [
                                "rbf"
                            ],
                            "C": [
                                0.5,
                                0.75,
                                1,
                                1.5
                            ],
                            "gamma": [
                                "scale"
                            ]
                        },
                        {
                            "kernel": [
                                "poly"
                            ],
                            "C": [
                                0.5,
                                0.75,
                                1,
                                1.5
                            ],
                            "gamma": [
                                "scale"
                            ],
                            "degree": [
                                3,
                                4,
                                5
                            ],
                            "coef0": [
                                0.0,
                                0.5,
                                1.0
                            ]
                        }
                    ],
                    "refit_metric": "root_mean_squared_error",
                    "n_jobs": -1
                }
            },
            "svr_scratch": {
                "model_name": "svr_scratch",
                "save_path": "../.models/svr_scratch",
                "metrics_path": "../.models/svr_scratch",
                "cv_statistics_path": "../.models/svr_scratch",
                "params": {
                    "kernel": "rbf",
                    "degree": 3,
                    "coef0": 0.0,
                    "tol": 1e-3,
                    "C": 1.0,
                    "epsilon": "auto",
                    "max_iter": 10000
                },
                "cv_params": {
                    "param_grid": [
                        {
                            "kernel": [
                                "linear"
                            ],
                            "C": [
                                0.5,
                                0.75,
                                1,
                                1.5
                            ]
                        },
                        {
                            "kernel": [
                                "rbf"
                            ],
                            "C": [
                                0.5,
                                0.75,
                                1,
                                1.5
                            ],
                            "gamma": [
                                "scale"
                            ]
                        },
                        {
                            "kernel": [
                                "poly"
                            ],
                            "C": [
                                0.5,
                                0.75,
                                1,
                                1.5
                            ],
                            "gamma": [
                                "scale"
                            ],
                            "degree": [
                                3,
                                4,
                                5
                            ],
                            "coef0": [
                                0.0,
                                0.5,
                                1.0
                            ]
                        }
                    ],
                    "refit_metric": "root_mean_squared_error",
                    "n_jobs": -1
                }
            },
            "rf_scratch": {
                "model_name": "rf_scratch",
                "save_path": "../.models/rf_scratch",
                "metrics_path": "../.models/rf_scratch",
                "cv_statistics_path": "../.models/rf_scratch",
                "params": {
                    "n_trees": 100,
                    "max_depth": 10,
                    "min_samples_split": 10,
                    "min_samples_leaf": 5,
                    "vote": "hard",
                    "impurity_metric": "gini",
                    "threshold": 0.5,
                    "seed": 42
                },
                "cv_params": {
                    "n_splits": 5,
                    "shuffle": true,
                    "random_state": 42,
                    "refit_metric": "f1",
                    "param_grid": {
                        "n_trees": [
                            50,
                            100
                        ],
                        "max_depth": [
                            10,
                            15
                        ],
                        "min_samples_split": [
                            5,
                            10
                        ],
                        "min_samples_leaf": [
                            2,
                            5
                        ],
                        "vote": [
                            "hard",
                            "soft"
                        ],
                        "impurity_metric": [
                            "gini",
                            "entropy"
                        ],
                        "threshold": [
                            0.4,
                            0.5,
                            0.6
                        ]
                    },
                    "n_jobs": -1
                }
            },
               "rf_scratch_eval": {
                "model_name": "rf_scratch",
                "params": {
                    "n_trees": 50,
                    "max_depth": 15,
                    "min_samples_split": 5,
                    "min_samples_leaf": 2,
                    "vote": "soft",
                    "impurity_metric": "entropy",
                    "threshold": 0.6,
                    "seed": 42
                },
                "cv_params": {
                    "n_splits": 5,
                    "shuffle": true,
                    "random_state": 42,
                    "refit_metric": "f1",
                    "param_grid": {},
                    "n_jobs": -1
                }
            },
            "rf_sklearn": {
                "model_name": "rf_sklearn",
                "save_path": "../.models/rf_sklearn",
                "metrics_path": "../.models/rf_sklearn",
                "cv_statistics_path": "../.models/rf_sklearn",
                "params": {
                    "n_estimators": 100,
                    "max_depth": 10,
                    "min_samples_split": 10,
                    "min_samples_leaf": 5,
                    "criterion": "gini",
                    "max_features": "sqrt",
                    "bootstrap": true,
                    "random_state": 42
                },
                "cv_params": {
                    "n_splits": 5,
                    "shuffle": true,
                    "random_state": 42,
                    "refit_metric": "f1",
                    "param_grid": {
                        "n_estimators": [
                            50,
                            100
                        ],
                        "max_depth": [
                            10,
                            15
                        ],
                        "min_samples_split": [
                            5,
                            10
                        ],
                        "min_samples_leaf": [
                            2,
                            5
                        ],
                        "criterion": [
                            "gini",
                            "entropy"
                        ]
                    },
                    "n_jobs": -1
                }
            },
            "rf_sklearn_eval": {
                "model_name": "rf_sklearn",
                "save_path": "../.models/rf_sklearn",
                "metrics_path": "../.models/rf_sklearn",
                "cv_statistics_path": "../.models/rf_sklearn",
                "params": {
                    "n_estimators": 100,
                    "max_depth": 15,
                    "min_samples_split": 5,
                    "min_samples_leaf": 2,
                    "criterion": "gini",
                    "max_features": "sqrt",
                    "bootstrap": true,
                    "random_state": 42
                },
                "cv_params": {
                    "n_splits": 5,
                    "shuffle": true,
                    "random_state": 42,
                    "refit_metric": "f1",
                    "param_grid": {},
                    "n_jobs": -1
                }
            }
        }
    }
}